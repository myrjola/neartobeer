{
  "name": "beersunderx",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint . --quiet",
    "test:unit": "jest",
    "test:integration": "mocha `find test -name '*spec.js'`"
  },
  "dependencies": {
    "appium": "^1.6.4",
    "asset-require-hook": "^1.0.2",
    "babel-plugin-module-resolver": "^1.6.0",
    "caching-transform": "^1.0.1",
    "google-polyline": "^1.0.0",
    "react": "16.0.0-alpha.6",
    "react-native": "^0.43.3",
    "react-native-maps": "^0.13.1",
    "react-native-radio-buttons": "^0.14.0",
    "react-native-router-flux": "^3.38.0",
    "react-native-vector-icons": "^4.0.1",
    "react-redux": "^5.0.4",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-mock-store": "^1.2.3",
    "redux-persist": "^4.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.10.0",
    "jest": "19.0.2",
    "mocha": "^3.2.0",
    "wd": "^1.2.0"
  },
  "jest": {
    "cacheDirectory": ".jest-cache",
    "haste": {
      "defaultPlatform": "ios",
      "platforms": [
        "android",
        "ios",
        "native"
      ],
      "providesModuleNodeModules": [
        "react-native"
      ]
    },
    "moduleNameMapper": {
      "^React$": "<rootDir>/node_modules/react"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^[./a-zA-Z0-9$_-]+\\.(bmp|gif|jpg|jpeg|png|psd|svg|webp)$": "<rootDir>/jest/mediaFileTransformer.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/react-native/Libraries/react-native/",
      "<rootDir>/node_modules/react-native/packager/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element)"
    ],
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "./jest/globalFetch.js",
      "./jest/globalNavigator.js",
      "./jest/mockReactNativeMaps.js"
    ]
  }
}

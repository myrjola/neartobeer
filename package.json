    "test:unit": "jest"
  },
  "dependencies": {
    "google-polyline": "^1.0.0",
    "react": "~15.4.1",
    "react-native": "0.42.3",
    "react-native-maps": "^0.13.1",
    "react-native-radio-buttons": "^0.14.0",
    "react-redux": "^5.0.3",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "3.18.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.10.0",
    "jest": "19.0.2",
